{
  "name": "pg-sink-VERTICAL",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "topics": "VERTICAL_historic",
    "connection.url": "jdbc:postgresql://iot-postgis:5432/DBCLIENT",
    "connection.user": "USER",
    "connection.password": "PASSWORD",
    "pk.fields": "timeinstant,entityid",
    "table.name.format": "DBCLIENT.${topic}",
    "auto.create": false,
    "auto.evolve": false,
    "pk.mode": "record_value",
    "insert.mode": "insert",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": true,
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "transforms": "timestampConvert, tRecvConvert",
    "transforms": "timestampConvert, recvtimeConvert",
    "transforms.timestampConvert.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.timestampConvert.field": "timeinstant",
    "transforms.timestampConvert.format": "yyyy-MM-dd'T'HH:mm:ss.SS'Z'",
    "transforms.timestampConvert.target.type": "Timestamp",
    "transforms.recvtimeConvert.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.recvtimeConvert.field": "recvtime",
    "transforms.recvtimeConvert.format": "yyyy-MM-dd'T'HH:mm:ss.SS'Z'",
    "transforms.recvtimeConvert.target.type": "Timestamp"
  }
}
